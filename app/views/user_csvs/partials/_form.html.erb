<%= form_with modal: @csv, url: user_csvs_path, method: :post, local: true do |f| %>
  <div class="row">
    <%= f.hidden_field :csv_cache %>

    <div class="col-12">
      <div class="custom-file">
        <%= f.file_field :csv, class: "custom-file-input" %>
        <%= f.label "Choose file", class: "custom-file-label" %>
      </div>
    </div>

    <div class="col-12 mt-3">
      <%= f.submit "Submit", class: "btn btn-danger" %>
    </div>
  </div>
<% end %>
